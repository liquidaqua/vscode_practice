
{
    "version": "2.0.0",
    "label": "g++ compile",
    "type": "shell",
    "command": "g++",
    "args": [
        "-std=gnu++1y",
        "-g",
        "-O2",
        "${fileBasename}",
        "-o",
        "${fileBasenameNoExtension}.exe"
    ],
    "group": {
        "kind": "build",
        "isDefault": true
    }
}